{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "expr": true,
    "immed": true,
    "indent": 2,
    "latedef": true,
    "newcap": false,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "smarttabs": true,
    "globals": {
        "angular": true,
        "expect": true,
        "confirm": true,
        "_": true,
        "toastr": true,
        "tinymce": true,
        "moment": true
    },
    "predef": [ // Extra globals.
        "define",
        "require",
        "exports",
        "module",
        "describe",
        "before",
        "beforeEach",
        "after",
        "afterEach",
        "it",
        "inject",
        "sinon",
        "node_env",
        "test_server",
        "test_client",
        "to"
    ]
}
